<style type="text/css">
    .field {
        border-radius: 3px;
        height: 35px;
        font-size: 18px;
    }

    .field-arrow {
        font-size: 26px;
    }

    .require-field {
        border: 1px solid red !important;
    }

    .my-custom-scrollbar {
        position: relative;
        height: 260px;
        overflow: auto;
    }

    .table-wrapper-scroll-y {
        display: block;
    }

    .text_area {
        resize: none !important;
    }

    .approved-bg {
        color: #1C84C6;
    }

    .cancel-bg {
        color: #EC4758;
    }

    .disapproved-bg {
        color: #D62728;
    }

    .new-bg {
        color: #1AB394;
    }

    .reviewed-bg {
        color: #00C5DA;
    }

    .submitted-bg {
        color: #F8AC59;
    }

    .sl-vl-radio-display-none {
        display: none;
    }

    .print_preview_iframe_css {
        border      : 1px solid ;
        overflow    : hidden;
        margin      : 0px auto;
        max-width   : 800px;
    }

    .print_preview_iframe_css2 {
        border: 0px none;
        margin-left: -270px;
        margin-top: -165px;
        height: 895px;
        width: 1075px;
    }

    span.details-control {
        background: url('../../ResourcesImages/show.jpg') no-repeat center center;
        background-size: 20px;
        width: 20px;
        height: 20px;
        cursor: pointer;
    }

    tr.shown span.details-control {
        background: url('../../ResourcesImages/unshow.jpg') no-repeat center center;
        background-size: 20px;
        width: 20px;
        height: 20px;
        cursor: pointer;
    }

    .badge {
        font-family: inherit;
        font-size: 10px;
        font-weight: 600;
        padding: 3px 8px;
        text-shadow: none;
    }
    .modal {
        overflow-y: auto;
    }

    .display-none 
    {
        display:none!important;
    }
</style>
